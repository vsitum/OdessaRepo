a1 = int(input('Write first number: \n'))
b1 = int(input('Write second number: \n'))
c1 = int(input('Write third number: \n'))
def lone_sum(a, b, c):
    if a == b and b != c:
        return c
    elif a == c and b != c:
        return b
    elif b == c and a != c:
        return a
    elif a == b and a == c and c == b:
        return 0
    else:
        return a + b + c
print(lone_sum(a1, b1, c1))

